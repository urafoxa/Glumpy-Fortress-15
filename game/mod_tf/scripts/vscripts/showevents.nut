if (!("ShowEventTable" in this))
	ShowEventTable <- {}
ShowEventTable.clear()

foreach (event in ShowEventList)
{
	local event_name = event
	ShowEventTable["OnGameEvent_" + event] <- function(params) 
	{
		if (ShowEvents)
		{
			printf("[EVENT] %g - %d - %s\n", Time(), GetFrameCount(), event_name)
			if (ShowEventParams)
			{
				foreach (k, v in params)
					printf(format("%12s - " + v + "\n", k))
			}
		}
	}
}
__CollectGameEventCallbacks(ShowEventTable)